<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PRG_MAUI_Car_Register.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PRG_MAUI_Car_Register.View"
    Shell.FlyoutBehavior="Flyout"
    Title="PRG_MAUI_Car_Register">

    <TabBar>
        <ShellContent Title="Start" ContentTemplate="{DataTemplate local:MainPage}"/>
        <ShellContent Title="Cars" ContentTemplate="{DataTemplate local:Car}"/>
        <ShellContent Title="Motorcycles" ContentTemplate="{DataTemplate local:Motorcycle}"/>
        <ShellContent Title="Trucks" ContentTemplate="{DataTemplate local:Truck}"/>
    </TabBar>
    

</Shell>
